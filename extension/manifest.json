// manifest.json
{
  "manifest_version": 3,
  "name": "Gimkit Code Spammer",
  "version": "1.0",
  "description": "A Chrome extension that spams codes for Gimkit games",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "background": {
    "service_worker": "backgroundScript.js"
  }
}
